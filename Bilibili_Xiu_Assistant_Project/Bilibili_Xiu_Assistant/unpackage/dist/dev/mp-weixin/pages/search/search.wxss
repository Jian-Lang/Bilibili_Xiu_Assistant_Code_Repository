
page{
	display: flex;
	flex-direction: column;
}
.logo{
	position: relative;
	margin-top: 35rpx;
	
	text-align: center;
	margin-bottom: -5rpx;
}
.logo image{
	width: 65%;
}
.searchInput{
	width: 100%;
}
input{
	text-align: center;
	margin: 0 auto;
	border-radius: 10rpx;
	border: 2rpx solid #000000;
	width: 95%;
	height: 70rpx;
}
.searchbtn{
	border-radius: 10rpx;
	background-color: #f27498;
	font-size: 33rpx;
	line-height: 65rpx;
	text-align: center;
	margin: 0 auto;
	margin-top: 60rpx;
	width: 35%;
	height: 65rpx;
	color: #FFFFFF;
}
.icon-search{
	margin-left: 5rpx;
}

