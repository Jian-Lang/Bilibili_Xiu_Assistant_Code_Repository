<view><view class="slogan"><image src="../../static/Protrait.png" mode="widthFix"></image></view><form><view class="cu-form-group"><view class="title">昵称:</view><input type="text" placeholder="请输入文明昵称..." name="userName" confirm-type="done" confirm-hold="true" placeholder-style="color:#000000" data-event-opts="{{[['input',[['__set_model',['','inputName','$event',[]]],['userNameInput',['$event']]]]]}}" value="{{inputName}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">密码:</view><input type="text" placeholder="请输入不少于8位的密码..." name="passWord" confirm-type="done" confirm-hold="true" placeholder-style="color:#000000" data-event-opts="{{[['input',[['__set_model',['','inputPwd','$event',[]]],['userPasswordInput',['$event']]]]]}}" value="{{inputPwd}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">Uid:</view><input type="text" placeholder="请输入B站Uid..." name="UID" confirm-type="done" confirm-hold="true" placeholder-style="color:#000000" data-event-opts="{{[['input',[['__set_model',['','inputUID','$event',[]]],['UIDInput',['$event']]]]]}}" value="{{inputUID}}" bindinput="__e"/></view></form><view class="padding"><button class="cu-btn block bg-blue margin-tb-sm lg" size="mini" disabled="{{hasPwd||hasName||hasUid}}" form-type="submit" data-event-opts="{{[['tap',[['subBtn_regis']]]]}}" bindtap="__e"><block wx:if="{{loadFlag}}"><text class="cuIcon-loading2 cuIconfont-spin"></text></block>确认注册</button></view><view class="padding"><button class="cu-btn block bg-blue margin-tb-sm lg" size="mini" data-event-opts="{{[['tap',[['goToNextPage']]]]}}" bindtap="__e"><block wx:if="{{loadFlag}}"><text class="cuIcon-loading2 cuIconfont-spin"></text></block>返回登录</button></view></view>